    "postinstall": "npm run drop-cache && npm run make",
    "make": "enb make",
    "drop-cache": "rm -rf .enb/tmp",
    "dev": "memcached || true & npm run drop-cache && npm run make && concurrently 'nodemon' 'parcel watch src/index.react.js --out-dir static/assets/react' ",
    "watch": "chokidar 'components/**' --initial -c 'enb make && notify -t bem-express -m Built'",
    "test": "npm run lint",
    "lint": "eslint {.,!src} && bemhint .",
    "stylelint": "stylelint static/assets/css/**/*.css",
    "prod": "npm run drop-cache && YENV=production npm run make && parcel build src/index.react.js --out-dir static/assets/react"
